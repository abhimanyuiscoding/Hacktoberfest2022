#include<bits/stdc++.h>
using namespace std;

int main()
{
	int i, j, n, temp, count=0;
	cout<<"Enter the size: ";
	cin>>n;
	int arr[n];
	
	cout<<"Enter the elements:\n";
	for(i=0; i<n; i++)
	{
		cin>>arr[i];
	}
	
	for(i=0; i<n-1; i++)
	{
		if(arr[i]==1)
		{
			if(arr[i+1]==0)
			{
				temp = arr[i];
				arr[i] = arr[i+1];
				arr[i+1] = temp;
			}
		}
		
		else if(arr[i]==2)
		{
			if(arr[i+1]==0 || arr[i+1]==1)
			{
				temp = arr[i];
				arr[i] = arr[i+1];
				arr[i+1] = temp;
			}
		}
 	}
	
	for(i=0; i<n; i++)
	{
		cout<<arr[i]<<"  ";
	}
}
